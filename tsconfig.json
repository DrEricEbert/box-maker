{
	"compilerOptions": {
		"noEmit": true,
		"allowJs": true,
		"checkJs": true,
		"baseUrl": ".",
		"jsx": "react",
		"lib": ["esnext", "dom"],
		"moduleResolution": "node",
		"paths": {
			"/*": ["./src/*"],
			"box-maker/*": ["./*"]
		}
	},
	"include": ["src/*.js"],
	"exclude": ["node_modules", "**/node_modules/*"]
}
